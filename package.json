{
  "name": "@relayin/base-provider",
  "version": "1.1.2",
  "description": "Base provider for Relay micro services",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/providers",
    "build/index.d.ts",
    "build/index.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "prepublishOnly": "npm run build",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "lint": "tslint --project tsconfig.json",
    "commit": "git-cz"
  },
  "keywords": [],
  "author": "relayin",
  "license": "UNLICENSED",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.0.3",
    "@types/node": "^12.0.4",
    "aws-sdk": "^2.468.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "fs-extra": "^8.0.1",
    "husky": "^2.3.0",
    "japa": "^2.0.10",
    "mrm": "^1.2.2",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1",
    "sqlite3": "^4.0.8",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@adonisjs/utils": "^1.4.4",
    "@types/got": "^9.4.4",
    "camelcase": "^5.3.1",
    "decamelize": "^3.2.0",
    "got": "^9.6.0",
    "igniculus": "^1.5.0",
    "indicative": "^7.1.1",
    "knex": "^0.17.3",
    "libphonenumber-js": "^1.7.18",
    "pg": "^7.11.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "aws-sdk": "^2.450.x"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js",
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md"
    }
  }
}
